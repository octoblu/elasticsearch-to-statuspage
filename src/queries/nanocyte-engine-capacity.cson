index: 'metric:nanocyte-engine-simple-2*'
body:
  size: 0
  from: 0
  aggs:
    recent:
      filter: bool:
        must: [ {
          range: date:
            gte: 'now-5m'
            lte: 'now'
            format: 'epoch_millis'
        } ]
        must_not: []
      aggs: 'byType':
        terms:
          field: 'type.raw'
          size: 5
          order: _term: 'desc'
        aggs: 'sumElapsedTime': sum: field: 'elapsedTime'
