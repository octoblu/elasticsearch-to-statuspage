index: 'metric:meshblu-core-dispatcher:sampled-2*'
body:
  size: 0
  from: 0
  aggs:
    recent:
      filter: bool:
        must: [ {
          range: date:
            gte: 'now-2m'
            lte: 'now-1m'
            format: 'epoch_millis'
        }
        {
          query: match: 'type.raw':
            query: 'meshblu-core-dispatcher:job'
            type: 'phrase'
        } ]
        must_not: []
      aggs: avg:
        avg:
          field: 'elapsedTime'
