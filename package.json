{
  "name": "octoblu-metrics-elasticsearch-to-statuspage",
  "version": "2.2.1",
  "description": "Report elasticsearch to statuspage",
  "main": "command.js",
  "scripts": {
    "test": "istanbul cover _mocha -- -R spec --require coffee-coverage/register-istanbul",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/elasticsearch-to-statuspage.git"
  },
  "author": "Octoblu, Inc. <npm@octoblu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/elasticsearch-to-statuspage/issues"
  },
  "homepage": "https://github.com/octoblu/elasticsearch-to-statuspage#readme",
  "dependencies": {
    "async": "^2.1.2",
    "dashdash": "^1.13.1",
    "debug": "^2.2.0",
    "elasticsearch": "^11.0.1",
    "fs-cson": "^0.3.2",
    "lodash": "^4.13.1",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.10.0",
    "istanbul": "^0.4.3",
    "mocha": "^3.1.2",
    "mocha-istanbul": "^0.3.0",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
